				,[ProductMinTerm]
				,[ProductMaxTerm]
				,[LoanPK]
				
    FROM
    (
		-- Product
SELECT DISTINCT CPM.XRP_ID [ProductPK]
	,CPM.XRP_Detail [Product]
	,SUBSTRING(CPM.XRP_Detail, (Charindex('\', CPM.XRP_Detail) + 1), LEN(CPM.XRP_Detail) - Charindex('\', CPM.XRP_Detail) + 1) [ProductShortName]
